{
  "servers": {
    "vision-bridge": {
      "type": "stdio",
      "command": "${workspaceFolder}/.mcp/vision-bridge/.venv/bin/python",
      "args": ["${workspaceFolder}/.mcp/vision-bridge/server.py"],
      "env": {
        "WORKSPACE_ROOT": "${workspaceFolder}",
        "OLLAMA_HOST": "http://localhost:11434",
        "OLLAMA_MODEL": "qwen2.5vl:7b",
        "VISION_MAX_EDGE": "1600",
        "VISION_TIMEOUT_S": "120"
      }
    }
  }
}
